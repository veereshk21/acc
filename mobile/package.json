{
  "name": "OneDigital",
  "version": "3.3.3",
  "description": "Mobile First",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "author": "Â© 2017 Verizon Wireless",
  "license": "MIT",
  "config": {
    "prodPublicPath": "http://localhost/build/"
  },
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "clean:all": "npm run analyze:clean && npm run bundle:clean",
    "clean:modules": "rimraf ./node_modules",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "build:prod": "npm run setup:np && npm run bundle:clean && node ./internals/webpack/bundle_page.js -b all",
    "bundle:prod": "npm run bundle:clean && cross-env NODE_ENV=production node ./internals/webpack/bundle_page.js -b all",
    "bundle:prod:page": "cross-env NODE_ENV=production node ./internals/webpack/bundle_page.js -b",
    "bundle:dev": "npm run bundle:clean && cross-env NODE_ENV=development node ./internals/webpack/bundle_page.js -b all",
    "bundle:dev:page": "cross-env NODE_ENV=development node ./internals/webpack/bundle_page.js -b",
    "check:pages": "node ./internals/webpack/bundle_page.js -p",
    "bundle:clean": "rimraf ./build",
    "start": "npm run build:dll && cross-env NODE_ENV=node_development node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk shelljs",
    "postsetup": "npm run build:dll",
    "postinstall": "npm run postsetup",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "setup:np": "npm run presetup && npm i",
    "setup": "npm run presetup && npm i",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore --ignore-pattern '!internals/scripts/**' --fix",
    "lint:js": "npm run lint:eslint -- .",
    "lint:staged": "lint-staged",
    "build:page": "node ./internals/webpack/bundle_page.js -h"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "es2015",
      "stage-2"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "plugins": [
      "redux-thunk",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "spread": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-restricted-syntax": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 0,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "radix": [
        "error",
        "as-needed"
      ]
    },
    "globals": {
      "__webpack_public_path__": true,
      "window": true,
      "document": true
    }
  },
  "fontsPath": "app/fonts",
  "imagesPath": "app/images",
  "jsonPath": "app/cq",
  "gNavPath": "gNav",
  "dependencies": {
    "axios": "0.17.1",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "focus-trap-react": "^3.1.1",
    "fontfaceobserver": "2.0.13",
    "history": "4.7.2",
    "html-react-parser": "0.4.1",
    "immutable": "3.8.2",
    "invariant": "2.2.2",
    "lodash": "4.17.4",
    "material-ui": "0.20.0",
    "no-scroll": "^2.1.0",
    "path": "0.12.7",
    "prop-types": "15.6.0",
    "qs": "6.5.1",
    "react": "16.2.0",
    "react-a11y": "1.0.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-aria-modal": "^2.12.0",
    "react-burger-menu": "2.2.3",
    "react-displace": "^2.3.0",
    "react-dom": "16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-id-swiper": "1.6.3",
    "react-lazyload": "2.3.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-render-html": "0.6.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-tap-event-plugin": "3.0.2",
    "redux": "3.7.2",
    "redux-form": "7.2.3",
    "redux-immutable": "4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "scriptjs": "2.5.8",
    "styled-components": "2.4.0",
    "warning": "3.0.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.2",
    "cheerio": "1.0.0-rc.2",
    "circular-dependency-plugin": "^5.0.1",
    "commander": "2.14.0",
    "compression": "1.7.1",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-react": "7.10.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.11",
    "git-branch": "1.0.0",
    "git-prepush-hook": "^1.0.2",
    "git-revision-webpack-plugin": "2.5.1",
    "glob": "7.1.2",
    "html-loader": "0.5.4",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.7.1",
    "ip": "1.1.5",
    "json-loader": "0.5.7",
    "lint-staged": "6.1.0",
    "minimist": "1.2.0",
    "node-sass": "4.8.3",
    "null-loader": "0.1.1",
    "offline-plugin": "4.9.0",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "pre-commit": "1.2.2",
    "psi": "3.1.0",
    "react-addons-test-utils": "15.6.2",
    "rimraf": "2.6.2",
    "sass-loader": "^6.0.7",
    "shelljs": "^0.8.1",
    "style-loader": "0.20.1",
    "url-loader": "1.0.1",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "worker-farm": "^1.6.0"
  }
}
